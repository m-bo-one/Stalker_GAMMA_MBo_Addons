<?xml version="1.0" encoding="UTF-8"?>
<game_dialogs>
	<dialog id="mbo_arena_totaliz_pre">
		<has_info>bar_arena_fight_8_done</has_info>
		<dont_has_info>mbo_arena_totaliz_pre</dont_has_info>
		<phrase_list>
			<phrase id="0">
				<text>mbo_arena_totaliz_pre_0</text>
				<next>1</next>
			</phrase>
			<phrase id="1">
				<text>mbo_arena_totaliz_pre_1</text>
				<give_info>mbo_arena_totaliz_pre</give_info>
			</phrase>
		</phrase_list>
	</dialog>
	<dialog id="mbo_arena_totaliz_non_grata">
		<precondition>mbo_arena_dialogs.totaliz_non_grata</precondition>
		<dont_has_info>totaliz_begin_over_dialog</dont_has_info>
		<has_info>mbo_arena_totaliz_pre</has_info>
		<phrase_list>
			<phrase id="0">
				<text>mbo_arena_totaliz_non_grata_0</text>
				<disable_info>totaliz_non_grata_over_dialog</disable_info>
				<next>1</next>
			</phrase>
			<phrase id="1">
				<text>mbo_arena_totaliz_non_grata_1</text>
				<next>2</next>
				<next>3</next>
			</phrase>
			<phrase id="2">
				<text>mbo_arena_totaliz_non_grata_2</text>
				<action>mbo_arena_dialogs.blow_to_face</action>
				<action>dialogs.break_dialog</action>
			</phrase>
			<phrase id="3">
				<text>mbo_arena_totaliz_non_grata_3</text>
				<next>4</next>
			</phrase>
			<phrase id="4">
				<text>mbo_arena_totaliz_non_grata_4</text>
			</phrase>
		</phrase_list>
	</dialog>
	<dialog id="mbo_arena_totaliz_begin">
		<precondition>mbo_arena_dialogs.totaliz_not_non_grata</precondition>
		<dont_has_info>totaliz_begin_over_dialog</dont_has_info> <!-- for spectator voices on arena, not sure that I want to change this -->
		<has_info>mbo_arena_totaliz_pre</has_info>
		<phrase_list>
			<phrase id="0">
				<text>mbo_arena_totaliz_begin_want_watch</text>
				<!-- bets -->
				<disable_info>mbo_arena_total_fight_is_over</disable_info>
				<disable_info>mbo_arena_remove_is_reload</disable_info>
				<disable_info>mbo_arena_totaliz_money_has_gone</disable_info>
				<disable_info>mbo_arena_totaliz_money_has_gone_5000</disable_info>
				<disable_info>mbo_arena_totaliz_money_has_gone_2500</disable_info>
				<disable_info>mbo_arena_totaliz_money_has_gone_1000</disable_info>

				<!-- fights -->
				<!-- !!!!! humans -->
				<disable_info>mbo_arena_total_fight_stalkers_vs_bandits</disable_info>

				<!-- stalkers -->
				<disable_info>mbo_arena_total_stalker_bet</disable_info>
				<disable_info>mbo_arena_total_stalker_done</disable_info>
				<disable_info>mbo_arena_total_stalker_1_death</disable_info>

				<!-- bandits -->
				<disable_info>mbo_arena_total_bandit_bet</disable_info>
				<disable_info>mbo_arena_total_bandit_done</disable_info>
				<disable_info>mbo_arena_total_bandit_1_death</disable_info>

				<!-- !!!!! monsters -->
				<disable_info>mbo_arena_total_fight_giant_vs_chimera</disable_info>
				<disable_info>mbo_arena_total_fight_pseudodog_vs_boar</disable_info>
				<disable_info>mbo_arena_total_fight_cats_vs_dogs</disable_info>

				<!-- giants -->
				<disable_info>mbo_arena_total_giant_bet</disable_info>
				<disable_info>mbo_arena_total_giant_done</disable_info>
				<disable_info>mbo_arena_total_giant_1_death</disable_info>

				<!-- pseudodog -->
				<disable_info>mbo_arena_total_pseudodog_bet</disable_info>
				<disable_info>mbo_arena_total_pseudodog_done</disable_info>
				<disable_info>mbo_arena_total_pseudodog_1_death</disable_info>

				<!-- dog -->
				<disable_info>mbo_arena_total_dog_bet</disable_info>
				<disable_info>mbo_arena_total_dog_done</disable_info>
				<disable_info>mbo_arena_total_dog_death</disable_info>
				<disable_info>mbo_arena_total_dog_1_death</disable_info>
				<disable_info>mbo_arena_total_dog_2_death</disable_info>
				<disable_info>mbo_arena_total_dog_3_death</disable_info>

				<!-- cat -->
				<disable_info>mbo_arena_total_cat_bet</disable_info>
				<disable_info>mbo_arena_total_cat_done</disable_info>
				<disable_info>mbo_arena_total_cat_death</disable_info>
				<disable_info>mbo_arena_total_cat_1_death</disable_info>
				<disable_info>mbo_arena_total_cat_2_death</disable_info>
				<disable_info>mbo_arena_total_cat_3_death</disable_info>

				<!-- chimeras -->
				<disable_info>mbo_arena_total_chimera_bet</disable_info>
				<disable_info>mbo_arena_total_chimera_done</disable_info>
				<disable_info>mbo_arena_total_chimera_1_death</disable_info>

				<!-- boar -->
				<disable_info>mbo_arena_total_boar_bet</disable_info>
				<disable_info>mbo_arena_total_boar_done</disable_info>
				<disable_info>mbo_arena_total_boar_1_death</disable_info>
				<disable_info>mbo_arena_total_boar_2_death</disable_info>

				<next>start</next>
			</phrase>
			<phrase id="start">
				<text>mbo_arena_totaliz_begin_choose</text>
				<next>s_to_s</next> <!-- stalker vs stalker -->
				<!-- <next>s_to_m</next> stalker vs monster -->
				<next>m_to_m</next> <!-- monster vs monster -->
				<next>exit_1</next> <!-- exit -->
			</phrase>

			<!-- START Stalker vs Stalker -->
			<phrase id="s_to_s">
				<text>mbo_arena_totaliz_begin_s_to_s</text>
				<next>s_to_s_select</next>
			</phrase>
			<phrase id="s_to_s_select">
				<text>mbo_arena_totaliz_begin_show_fights</text>
				<next>stalkers_vs_bandits</next>
				<next>exit_2</next>
			</phrase>

				<!-- Stalkers vs Bandits -->
				<phrase id="stalkers_vs_bandits">
					<text>mbo_arena_totaliz_begin_stalkers_vs_bandits</text>
					<next>stalkers_vs_bandits_bet</next>
				</phrase>
				<phrase id="stalkers_vs_bandits_bet">
					<text>mbo_arena_totaliz_begin_bet_ask</text>
					<next>stalkers_vs_bandits_bet_on_1</next>
					<next>stalkers_vs_bandits_bet_on_2</next>
					<next>exit_2</next>
				</phrase>
				<phrase id="stalkers_vs_bandits_bet_on_1">
					<text>mbo_arena_totaliz_begin_bet_stalkers</text>
					<give_info>mbo_arena_total_fight_stalkers_vs_bandits</give_info>
					<give_info>mbo_arena_total_stalker_bet</give_info>
					<next>bet_amt</next>
				</phrase>
				<phrase id="stalkers_vs_bandits_bet_on_2">
					<text>mbo_arena_totaliz_begin_bet_bandits</text>
					<give_info>mbo_arena_total_fight_stalkers_vs_bandits</give_info>
					<give_info>mbo_arena_total_bandit_bet</give_info>
					<next>bet_amt</next>
				</phrase>
			<!-- END Stalker vs Stalker -->

			<!-- START Monster Vs Monster -->
			<phrase id="m_to_m">
				<text>mbo_arena_totaliz_begin_m_to_m</text>
				<next>m_to_m_select</next>
			</phrase>
			<phrase id="m_to_m_select">
				<text>mbo_arena_totaliz_begin_show_fights</text>
				<!-- <next>g_vs_ch</next> -->
				<next>pseudodog_vs_boar</next>
				<next>cats_vs_dogs</next>
				<next>exit_2</next>
			</phrase>

				<!-- Giant vs chimera -->
				<phrase id="g_vs_ch">
					<text>mbo_arena_totaliz_begin_g_vs_ch</text>
					<next>g_vs_ch_bet</next>
				</phrase>
				<phrase id="g_vs_ch_bet">
					<text>mbo_arena_totaliz_begin_bet_ask</text>
					<next>g_vs_ch_bet_on_1</next>
					<next>g_vs_ch_bet_on_2</next>
					<next>exit_2</next>
				</phrase>
				<phrase id="g_vs_ch_bet_on_1">
					<text>mbo_arena_totaliz_begin_bet_giant</text>
					<give_info>mbo_arena_total_fight_giant_vs_chimera</give_info>
					<give_info>mbo_arena_total_giant_bet</give_info>
					<next>bet_amt</next>
				</phrase>
				<phrase id="g_vs_ch_bet_on_2">
					<text>mbo_arena_totaliz_begin_bet_chimera</text>
					<give_info>mbo_arena_total_fight_giant_vs_chimera</give_info>
					<give_info>mbo_arena_total_chimera_bet</give_info>
					<next>bet_amt</next>
				</phrase>

				<!-- Pseudodog vs boar -->
				<phrase id="pseudodog_vs_boar">
					<text>mbo_arena_totaliz_begin_pseudodog_vs_boar</text>
					<next>pseudodog_vs_boar_bet</next>
				</phrase>
				<phrase id="pseudodog_vs_boar_bet">
					<text>mbo_arena_totaliz_begin_bet_ask</text>
					<next>pseudodog_vs_boar_bet_on_1</next>
					<next>pseudodog_vs_boar_bet_on_2</next>
					<next>exit_2</next>
				</phrase>
				<phrase id="pseudodog_vs_boar_bet_on_1">
					<text>mbo_arena_totaliz_begin_bet_pseudo</text>
					<give_info>mbo_arena_total_fight_pseudodog_vs_boar</give_info>
					<give_info>mbo_arena_total_pseudodog_bet</give_info>
					<next>bet_amt</next>
				</phrase>
				<phrase id="pseudodog_vs_boar_bet_on_2">
					<text>mbo_arena_totaliz_begin_bet_boar</text>
					<give_info>mbo_arena_total_fight_pseudodog_vs_boar</give_info>
					<give_info>mbo_arena_total_boar_bet</give_info>
					<next>bet_amt</next>
				</phrase>

				<!-- Cats vs dogs -->
				<phrase id="cats_vs_dogs">
					<text>mbo_arena_totaliz_begin_cats_vs_dogs</text>
					<next>cats_vs_dogs_bet</next>
				</phrase>
				<phrase id="cats_vs_dogs_bet">
					<text>mbo_arena_totaliz_begin_bet_ask</text>
					<next>cats_vs_dogs_bet_on_1</next>
					<next>cats_vs_dogs_bet_on_2</next>
					<next>exit_2</next>
				</phrase>
				<phrase id="cats_vs_dogs_bet_on_1">
					<text>mbo_arena_totaliz_begin_bet_cats</text>
					<give_info>mbo_arena_total_fight_cats_vs_dogs</give_info>
					<give_info>mbo_arena_total_cat_bet</give_info>
					<next>bet_amt</next>
				</phrase>
				<phrase id="cats_vs_dogs_bet_on_2">
					<text>mbo_arena_totaliz_begin_bet_dogs</text>
					<give_info>mbo_arena_total_fight_cats_vs_dogs</give_info>
					<give_info>mbo_arena_total_dog_bet</give_info>
					<next>bet_amt</next>
				</phrase>
			<!-- END Monster Vs Monster -->

			<!-- Bets -->
			<phrase id="bet_amt">
				<text>mbo_arena_totaliz_begin_bet_amt</text>
				<next>bet_1000</next>
				<next>bet_2500</next>
				<next>bet_5000</next>
				<next>exit_no_money</next>
			</phrase>
			<phrase id="bet_1000">
				<precondition>dialogs_mlr.totaliz_1</precondition>
				<text>mbo_arena_totaliz_begin_bet_1000</text>
				<give_info>mbo_arena_begin_the_battle</give_info>
				<give_info>mbo_arena_totaliz_money_has_gone</give_info>
				<give_info>mbo_arena_totaliz_money_has_gone_1000</give_info>
				<action>dialogs_mlr.take_totaliz_1</action>
				<give_info>totaliz_begin_over_dialog</give_info>
				<disable_info>total_reward_over_dialog</disable_info>
				<action>dialogs_mlr.teleport_for_totaliz</action>
				<action>dialogs.break_dialog</action>
			</phrase>
			<phrase id="bet_2500">
				<precondition>dialogs_mlr.totaliz_2</precondition>
				<text>mbo_arena_totaliz_begin_bet_2500</text>
				<give_info>mbo_arena_begin_the_battle</give_info>
				<give_info>mbo_arena_totaliz_money_has_gone</give_info>
				<give_info>mbo_arena_totaliz_money_has_gone_2500</give_info>
				<action>dialogs_mlr.take_totaliz_2</action>
				<give_info>totaliz_begin_over_dialog</give_info>
				<disable_info>total_reward_over_dialog</disable_info>
				<action>dialogs_mlr.teleport_for_totaliz</action>
				<action>dialogs.break_dialog</action>
			</phrase>
			<phrase id="bet_5000">
				<precondition>dialogs_mlr.totaliz_5</precondition>
				<text>mbo_arena_totaliz_begin_bet_5000</text>
				<give_info>mbo_arena_begin_the_battle</give_info>
				<give_info>mbo_arena_totaliz_money_has_gone</give_info>
				<give_info>mbo_arena_totaliz_money_has_gone_5000</give_info>
				<action>dialogs_mlr.take_totaliz_5</action>
				<give_info>totaliz_begin_over_dialog</give_info>
				<disable_info>total_reward_over_dialog</disable_info>
				<action>dialogs_mlr.teleport_for_totaliz</action>
				<action>dialogs.break_dialog</action>
			</phrase>
			<!-- exits -->
			<phrase id="exit_1">
				<text>mbo_arena_totaliz_begin_exit_1</text>
			</phrase>
			<phrase id="exit_2">
				<text>mbo_arena_totaliz_begin_exit_2</text>
			</phrase>
			<phrase id="exit_no_money">
				<text>mbo_arena_totaliz_begin_no_money</text>
			</phrase>
		</phrase_list>
	</dialog>
	<dialog id="mbo_arena_totaliz_bet_lost">
		<dont_has_info>total_reward_over_dialog</dont_has_info>
		<has_info>mbo_arena_total_fight_is_over</has_info>
		<precondition>mbo_arena_dialogs.totaliz_bet_lost</precondition>
		<precondition>mbo_arena_dialogs.totaliz_bet_exist</precondition>
		<phrase_list>
			<phrase id="0">
				<text>mbo_arena_totaliz_begin_have_prize</text>
				<disable_info>totaliz_begin_over_dialog</disable_info>
				<next>totaliz_death</next>
			</phrase>
			<phrase id="totaliz_death">
				<text>mbo_arena_totaliz_begin_bet_lost</text>
				<give_info>total_reward_over_dialog</give_info>
				<action>mbo_arena_dialogs.total_clear_info</action>
			</phrase>
		</phrase_list>
	</dialog>
	<dialog id="mbo_arena_totaliz_bet_win">
		<dont_has_info>total_reward_over_dialog</dont_has_info>
		<has_info>mbo_arena_total_fight_is_over</has_info>
		<precondition>mbo_arena_dialogs.totaliz_bet_win</precondition>
		<precondition>mbo_arena_dialogs.totaliz_bet_exist</precondition>
		<phrase_list>
			<phrase id="0">
				<text>mbo_arena_totaliz_begin_have_prize</text>
				<disable_info>totaliz_begin_over_dialog</disable_info>
				<next>totaliz_win</next>
			</phrase>
			<phrase id="totaliz_win">
				<text>mbo_arena_totaliz_begin_take_prize</text>
				<give_info>total_reward_over_dialog</give_info>
				<action>mbo_arena_dialogs.total_dynamic_reward</action>
			</phrase>
		</phrase_list>
	</dialog>
</game_dialogs>
